{
  "version": 2,
  "installCommand": "npm install && PLAYWRIGHT_BROWSERS_PATH=0 npx playwright install chromium",
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/index.ts": {
      "includeFiles": "node_modules/playwright-core/.local-browsers/**",
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "env": {
    "PLAYWRIGHT_BROWSERS_PATH": "0"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ]
}
